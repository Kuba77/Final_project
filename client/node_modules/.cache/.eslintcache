om \"./ProductDetails/ProductDetails\";\nimport ProductImg from \"./ProductImg/ProductImg\";\nimport classes from \"./Product.module.scss\";\nimport Button from \"../Button/Button\";\n\nconst Product = () => {\n  let { productId } = useParams();\n  const dispatch = useDispatch();\n  const [product, setProduct] = useState({});\n  const [toggle, setToggle] = useState(0);\n\n  function addToCart(info) {\n    try {\n      dispatch(setItemInCart(info));\n    } catch (error) {\n      console.error(error.message);\n    }\n  }\n\n  const getProduct = useCallback(async () => {\n    const products = await getSelectedProduct(productId);\n    setProduct(products);\n  }, [setProduct]);\n\n  useEffect(() => {\n    getProduct();\n  }, []);\n\n  return (\n    <React.Fragment>\n      {!!product.name && (\n        <div>\n          <div className={classes.product__header}>\n            <p>\n              <Link to=\"/\">Home</Link>/ <Link to=\"/products\">Books</Link>/\n              {product.name}\n            </p>\n          </div>\n\n          <div className={classes.product_block}>\n            <div className={classes.product_block__main}>\n              <div className={classes.title_block}>\n                <ProductTitle\n                  className={classes.product_info__title}\n                  title={product.name}\n                />\n                <ProductAutor\n                  className={classes.product_info__author}\n                  author={product.author}\n                />\n              </div>\n\n              <div className={classes.product_info}>\n                <ProductDescription\n                  className={classes.product_info__text}\n                  description={product.description}\n                />\n                <ProductPriceBlock product={product} />\n\n                <div className={classes.product_button}>\n                  <Button\n                    type=\"main\"\n                    onClick={() => {\n                      addToCart(product);\n                    }}\n                  >\n                   <BsBasket color=\"white\" size={26} />\n                  </Button>\n\n                  <Button type=\"main\">\n                  <BsFillHeartFill color=\"white\" size={26} />\n                  </Button>\n                </div>\n              </div>\n\n              <div className={classes.product_img}>\n                <div className={classes.product_img__large}>\n                  <ProductImg\n                    className={classes.gallery_item__large}\n                    item={product.imageUrls[toggle]}\n                    alt={product.name}\n                  />\n                </div>\n\n                <div className={classes.gallery}>\n                  {product.imageUrls.map((item, index) => (\n                    <ProductImg\n                      key={index}\n                      className={classes.gallery__item}\n                      item={item}\n                      alt={product.name}\n                      onClick={() => setToggle(index)}\n                    />\n                  ))}\n                </div>\n              </div>\n            </div>\n\n            <h3 className={classes.product_block__title}>Details</h3>\n\n            <ProductDetails product={product} />\n\n            <h3 className={classes.product_block__title} id=\"review\">\n              Customer Reviews\n            </h3>\n\n            <div className={classes.product_block__review}>\n              <form>\n                <textarea\n                  id=\"review\"\n                  name=\"Textarea\"\n                  placeholder=\"Please, live your comment here...\"\n                ></textarea>\n                <div className={classes.review__buttons}>\n                  <Button type=\"main\" size=\"m\">\n                    Reset\n                  </Button>\n                  <Button type=\"main\" size=\"m\">\n                    Send\n                  </Button>\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n      )}\n    </React.Fragment>\n  );\n};\n\nexport default Product;","/home/tirion/Desktop/Final_project/client/src/api/productsApi.js",[],"/home/tirion/Desktop/Final_project/client/src/api/userApi.js",[],"/home/tirion/Desktop/Final_project/client/src/store/selectors.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Categories/CategoriesList.js",["295"],"import React, { useCallback, useState, useEffect } from \"react\";\nimport CategoriesItem from \"./CategoriesItem\";\nimport { getAllCategories } from \"../../api/catalogApi\";\n\nconst CategoriesList = () => {\n  const [category, setCategory] = useState([]);\n  const getCategories = useCallback(async () => {\n    const categories = await getAllCategories();\n    setCategory(categories);\n  }, [setCategory]);\n\n  useEffect(() => {\n    getCategories();\n  }, []);\n\n  return (\n    <>\n      {category.map((item) => (\n        <CategoriesItem item={item} key={item.id} />\n      ))}\n    </>\n  );\n};\n\nexport default CategoriesList;\n","/home/tirion/Desktop/Final_project/client/src/components/Cart/CartItemHeader.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Cart/CartProduct.js",["296"],"import React from \"react\";\nimport classes from \"./cart.module.scss\"\nimport {MdRemoveShoppingCart} from \"react-icons/md\"\n\nconst CartProduct = () => {\n    return(\n        <div className={classes.block_product}>\n            <div className={classes.product_img_and_text}>\n                <a className={classes.product_img} href=\"#\">\n                    <img className={classes.product_img} src=\"#\" alt=\"product\" />\n                </a>\n                <div className={classes.product_text}>\n                    <p className={classes.product_article}>article</p>\n                    <h3 className={classes.product_title}>\n                        title\n                    </h3>\n                    <p className={classes.product_description}>\n                        description\n                    </p>\n                </div>\n            </div>\n                <div className={classes.product_culc}>\n                    <div className={classes.cart_button_prod}>\n                        <div className={classes.cart_button__counter_prod}>\n                            <button className={classes.cart_button_minus}>-</button>\n                            <span className={classes.cart_counter_span}>1</span>\n                            <button className={classes.cart_button_plus}>+</button>\n                        </div>                           \n                    </div>\n                </div>\n                <div className={classes.product_price}>\n                    <p>$</p>\n                    <p>25</p>\n                </div>\n                <div className={classes.product_total_price}>\n                    <p>$</p>\n                    <p>50</p>\n                    <MdRemoveShoppingCart className={classes.product_remove_icon} />\n                </div>\n        </div>\n    )\n}\n\nexport default CartProduct;","/home/tirion/Desktop/Final_project/client/src/components/Footer/FooterCustomerAreaLinks.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Footer/SubscribeBlock.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Footer/FooterQuickLinks.js",[],"/home/tirion/Desktop/Final_project/client/src/api/api.js",[],"/home/tirion/Desktop/Final_project/client/src/hooks/useWindowSize.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Header/Basket/Basket.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Header/Nav/Nav.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Header/Logo/Logo.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Button/Button.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Feature/Carousel/Carousel.jsx",[],"/home/tirion/Desktop/Final_project/client/src/components/Categories/CategoriesItem.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Footer/FooteLink.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Footer/FormSubscribe.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Header/Basket/Counter/Counter.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/CollectionPage/CollectionList/CollectionList.js",[],"/home/tirion/Desktop/Final_project/client/src/components/BookItem/BookItem.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/CollectionPage/CollectionPage.js",["297"],"import React, { useEffect, useState, useCallback } from \"react\";\nimport Header from \"../../components/Header/Header\";\nimport classes from \"./CollectionPage.module.scss\";\nimport PuffLoader from \"react-spinners/PuffLoader\";\nimport { getAllProducts } from \"../../api/productsApi\";\nimport CollectionList from \"./CollectionList/CollectionList\";\nimport Pagination from \"./Pagination/Pagination\";\n\nimport Filters from \"../../components/Filters/Filters\";\nimport { chekingArray } from \"../../utils/utils\";\nimport { getFilteredProductByQuery } from \"../../api/productsApi\";\n\nconst CollectionPage = () => {\n  const [collection, setCollection] = useState([]);\n  const [isLoading, setLoading] = useState(false);\n  const [currentPage, setCurrentPage] = useState(1);\n  const [productsInPage] = useState(15);\n\n  const [genderSelected, setgenderSelected] = useState([]);\n\n  function getselectedGenre(value) {\n    const selected = chekingArray(genderSelected, value);\n    console.log(selected);\n    setgenderSelected(selected);\n  }\n\n  const getGenderProducts = useCallback(\n    async (value) => {\n      let params = new URLSearchParams();\n      params.append(\"genre\", value);\n      let string = params.toString();\n      const products = await getFilteredProductByQuery(string);\n      setCollection(products.products);\n    },\n    [setCollection]\n  );\n\n  useEffect(() => {\n    let GenString = genderSelected.join();\n    getGenderProducts(GenString);\n  }, [genderSelected]);\n\n  useEffect(() => {\n    const getCollection = async () => {\n      setLoading(true);\n      const response = await getAllProducts();\n      console.log(response);\n      setCollection(response);\n      setLoading(false);\n    };\n    getCollection();\n  }, []);\n\n  const lastProductIndex = currentPage * productsInPage;\n  const firstProductIndex = lastProductIndex - productsInPage;\n  const currentProduct = collection.slice(firstProductIndex, lastProductIndex);\n\n  const paginate = (pageNumber, setLoading) => {\n    setCurrentPage(pageNumber);\n  };\n\n  return (\n    <>\n      <Header />\n      <section className={classes.collection}>\n        {isLoading && (\n          <div className={classes.collection__loader}>\n            <PuffLoader loading={isLoading} color=\"purple\" size={120} />\n          </div>\n        )}\n\n        <Filters onChange={getselectedGenre} />\n\n        <div className={classes.collection__container}>\n          {!isLoading && <CollectionList collection={currentProduct} />}\n        </div>\n\n        <Pagination\n          productsInPage={productsInPage}\n          totalProducts={collection.length}\n          paginate={paginate}\n        />\n      </section>\n    </>\n  );\n};\n\nexport default CollectionPage;\n","/home/tirion/Desktop/Final_project/client/src/pages/category-page/CategoryPage.jsx",["298","299"],"import React, { useEffect, useState, useCallback } from \"react\";\n\nimport { useParams, Link } from \"react-router-dom\";\n\nimport { getFilteredProductByCategory } from \"../../api/productsApi\";\n\nfunction CategoryPage() {\n  let { categoryId } = useParams();\n  const [product, setProduct] = useState([]);\n\n  const getProducts = useCallback(async () => {\n    const products = await getFilteredProductByCategory(categoryId);\n    console.log(products);\n    setProduct(products.products);\n  }, [setProduct]);\n\n  useEffect(() => {\n    getProducts();\n  }, []);\n\n  return (\n    <div>\n      <h1>{categoryId}</h1>\n      {product.map((item) => (\n        <Link id={item.itemNo} key={item.itemNo} to={`/product/${item.itemNo}`}>\n          <h5>{item.name}</h5>\n        </Link>\n      ))}\n    </div>\n  );\n}\n\nexport default CategoryPage;\n","/home/tirion/Desktop/Final_project/client/src/components/SearchBar/SearchBar.jsx",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductAutor/ProductAutor.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductDescription/ProductDescription.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductTitle/ProductTitle.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductPriceBlock/ProductPriceBlock.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductDetails/ProductDetails.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductImg/ProductImg.js",[],"/home/tirion/Desktop/Final_project/client/src/api/catalogApi.js",[],"/home/tirion/Desktop/Final_project/client/src/hooks/useDebounce.js",["300"],"import { useState, useEffect } from \"react\";\n\nexport function useDebounce(value, timeout, callback) {\n  const [timer, setTimer] = useState(null);\n  const clearTimer = () => {\n    if (timer) clearTimeout(timer);\n  };\n\n  useEffect(() => {\n    clearTimer();\n\n    if (value && callback) {\n      const newTimer = setTimeout(callback, timeout);\n      setTimer(newTimer);\n    }\n  }, [value]);\n}\n","/home/tirion/Desktop/Final_project/client/src/pages/CollectionPage/Pagination/Pagination.js",["301"],"import React from \"react\";\nimport classes from \"./pagination.module.scss\";\nconst Pagination = ({ productsInPage, totalProducts, paginate}) => {\n  const pageNumbers = [];\n\n  for (let i = 1; i <= Math.ceil(totalProducts / productsInPage); i++) {\n    pageNumbers.push(i);\n  }\n  return (\n    <div className={classes.pagination_buttons}>\n      <ul className={classes.pagination_buttons__container}>\n        {pageNumbers.map((number) => (\n          <li key={number} className={classes.pagination_buttons__item}>\n            <a href=\"#\" onClick={() => paginate(number)}>\n              {number}\n            </a>\n          </li>\n        ))}\n      </ul>\n    </div>\n  );\n};\n\nexport default Pagination;\n","/home/tirion/Desktop/Final_project/client/src/components/Product/ProductStore/ProductStore.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/ProductPrice/ProductPrice.js",[],"/home/tirion/Desktop/Final_project/client/src/components/SearchBar/SearchContent/SearchContent.jsx",[],"/home/tirion/Desktop/Final_project/client/src/components/Forms/LoginForm.jsx",["302"],"import React, { useCallback } from \"react\";\nimport { Formik, Form } from \"formik\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport FormikControl from \"./FormikControl\";\nimport classes from \"./Form.module.scss\";\nimport { useDispatch } from \"react-redux\";\nimport { setCustomer } from \"../../store/customer/reducer\";\nimport { setErors } from \"../../store/errors/reducer\";\nimport { logOrRegisterCustomer } from \"../../api/userApi\";\nimport { GoogleLogin } from \"react-google-login\";\nimport configData from \"../../config/config.json\";\n\nfunction LoginForm(props) {\n  const { initialValues, validationSchema, onSubmit } = props;\n\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const responseSuccessGoogle = useCallback(\n    async (response) => {\n      try {\n        let customer = await logOrRegisterCustomer(response);\n        if (customer.message) {\n          dispatch(setErors(customer.message));\n        } else {\n          dispatch(setCustomer(customer));\n          history.push(\"/\");\n        }\n      } catch (error) {\n        dispatch(setErors(error.response));\n      }\n    },\n    [dispatch]\n  );\n  const responseErrorGoogle = useCallback(\n    async (response) => {\n      dispatch(setErors(response.message));\n    },\n    [dispatch]\n  );\n\n  return (\n    <Formik\n      initialValues={initialValues}\n      validationSchema={validationSchema}\n      onSubmit={onSubmit}\n    >\n      {(formik) => {\n        return (\n          <div className={classes.form__wrapper}>\n            <h1>Login form</h1>\n            <Form>\n              <FormikControl\n                control=\"input\"\n                type=\"email\"\n                label=\"Email\"\n                name=\"email\"\n              />\n              <FormikControl\n                control=\"input\"\n                type=\"password\"\n                label=\"Password\"\n                name=\"password\"\n              />\n              <div className={classes.button__wrapper}>\n                <button\n                  className={classes.form__btn}\n                  type=\"submit\"\n                  disabled={!formik.isValid}\n                >\n                  Submit\n                </button>\n                <GoogleLogin\n                  clientId={configData.REACT_APP_GOOGLE_CLIENT_ID}\n                  buttonText=\"Login with google\"\n                  onSuccess={responseSuccessGoogle}\n                  onFailure={responseErrorGoogle}\n                  cookiePolicy={\"single_host_origin\"}\n                />\n                <Link className={classes.form__link} to=\"/registration\">\n                  Go to register page\n                </Link>\n              </div>\n            </Form>\n          </div>\n        );\n      }}\n    </Formik>\n  );\n}\n\nexport default LoginForm;\n","/home/tirion/Desktop/Final_project/client/src/components/Forms/RegistrationForm.jsx",["303"],"import React, { useCallback } from \"react\";\nimport { Formik, Form } from \"formik\";\nimport FormikControl from \"./FormikControl\";\nimport classes from \"./Form.module.scss\";\nimport { useDispatch } from \"react-redux\";\nimport { setCustomer} from \"../../store/customer/reducer\";\nimport { setErors } from \"../../store/errors/reducer\";\nimport { logOrRegisterCustomer } from \"../../api/userApi\";\nimport { GoogleLogin } from \"react-google-login\";\nimport configData from \"../../config/config.json\";\nimport TextError from \"./components/TextError\";\nimport { useHistory } from \"react-router-dom\";\n\nfunction RegistrationForm(props) {\n  const { initialValues, validationSchema, onSubmit, errorMessage } = props;\n\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const responseSuccessGoogle = useCallback(\n    async (response) => {\n      try {\n        let customer = await logOrRegisterCustomer(response);\n        if (customer.message) {\n          dispatch(setErors(customer.message));\n        } else {\n          dispatch(setCustomer(customer));\n          history.push(\"/\");\n        }\n      } catch (error) {\n        dispatch(setErors(error.response));\n      }\n    },\n    [dispatch]\n  );\n  const responseErrorGoogle = useCallback(\n    async (response) => {\n      dispatch(setErors(response.message));\n    },\n    [dispatch]\n  );\n\n  return (\n    <Formik\n      initialValues={initialValues}\n      validationSchema={validationSchema}\n      onSubmit={onSubmit}\n    >\n      {(formik) => {\n        return (\n          <div className={classes.form__wrapper}>\n            <h1>Registration form</h1>\n            <Form>\n              <FormikControl\n                control=\"input\"\n                type=\"text\"\n                label=\"Please, enter your first name\"\n                name=\"firstName\"\n              />\n              <FormikControl\n                control=\"input\"\n                type=\"text\"\n                label=\"Please, enter your last name\"\n                name=\"lastName\"\n              />\n              <FormikControl\n                control=\"input\"\n                type=\"text\"\n                label=\"Please, enter your login\"\n                name=\"login\"\n              />\n              <FormikControl\n                control=\"input\"\n                type=\"email\"\n                label=\"Email\"\n                name=\"email\"\n              />\n              <TextError>{errorMessage}</TextError>\n              <FormikControl\n                control=\"input\"\n                type=\"password\"\n                label=\"Password\"\n                name=\"password\"\n              />\n              <FormikControl\n                control=\"input\"\n                type=\"password\"\n                label=\"Confirm Password\"\n                name=\"confirmPassword\"\n              />\n              <div className={classes.button__wrapper}>\n                <button\n                  className={classes.form__btn}\n                  type=\"submit\"\n                  disabled={!formik.isValid}\n                >\n                  Submit\n                </button>\n                {/* <button\n                className=\"form__btn\"\n                type=\"button\"\n                onClick={() => { dispatch(removeCustomer()); }}\n              >\n                LOGOUT\n              </button> */}\n                <GoogleLogin\n                  clientId={configData.REACT_APP_GOOGLE_CLIENT_ID}\n                  buttonText=\"Register with google\"\n                  onSuccess={responseSuccessGoogle}\n                  onFailure={responseErrorGoogle}\n                  cookiePolicy={\"single_host_origin\"}\n                />\n              </div>\n            </Form>\n          </div>\n        );\n      }}\n    </Formik>\n  );\n}\n\nexport default RegistrationForm;\n","/home/tirion/Desktop/Final_project/client/src/components/Forms/FormikControl.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Forms/ValidationSchema.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Forms/components/TextError.js",[],"/home/tirion/Desktop/Final_project