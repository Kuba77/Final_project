ges.js",[],"/home/tirion/Desktop/Final_project/client/src/store/index.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/HomePage/HomePage.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/CartPage/CartPage.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/ProductPage/ProductPage.js",[],"/home/tirion/Desktop/Final_project/client/src/pages/RegistrationPage/RegistrationPage.jsx",[],"/home/tirion/Desktop/Final_project/client/src/pages/LoginPage/LoginPage.jsx",["260"],"/home/tirion/Desktop/Final_project/client/src/store/customer/reducer.js",[],"/home/tirion/Desktop/Final_project/client/src/store/cart/reducer.js",[],"/home/tirion/Desktop/Final_project/client/src/store/errors/reducer.js",[],"/home/tirion/Desktop/Final_project/client/src/store/favorites/reducer.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Header/Header.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Categories/Categories.js",["261","262"],"import React, { useCallback, useState } from \"react\";\nimport classes from \"./Categories.module.scss\";\nimport CategoriesList from \"./CategoriesList\";\n\nconst Categories = () => {\n  return (\n    <>\n      <div className={classes.categories_wrapper}>\n        <div className={classes.categories_block}>\n          <h1 className={classes.categories_title}>Categories</h1>\n          <div className={classes.categories_module}>\n            <CategoriesList />\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Categories;\n","/home/tirion/Desktop/Final_project/client/src/components/Cart/CartProductList.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Cart/CartHeader.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Main/Main.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Cart/CartSummary.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Feature/Feature.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Footer/Footer.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Product/Product.js",["263","264"],"import React, { useEffect, useState, useCallback } from \"react\";\nimport { useParams, Link } from \"react-router-dom\";\nimport { getSelectedProduct } from \"../../api/productsApi\";\nimport { useDispatch } from \"react-redux\";\nimport { setItemInCart } from \"../../store/cart/reducer\";\nimport ProductTitle from \"./ProductTitle/ProductTitle\";\nimport ProductAutor from \"./ProductAutor/ProductAutor\";\nimport ProductDescription from \"./ProductDescription/ProductDescription\";\nimport ProductPriceBlock from \"./ProductPriceBlock/ProductPriceBlock\";\nimport ProductDetails from \"./ProductDetails/ProductDetails\";\nimport ProductImg from \"./ProductImg/ProductImg\";\nimport classes from \"./Product.module.scss\";\nimport Button from \"../Button/Button\";\n\nconst Product = () => {\n  let { productId } = useParams();\n  const dispatch = useDispatch();\n  const [product, setProduct] = useState({});\n  const [toggle, setToggle] = useState(0);\n\n  function addToCart(info) {\n    try {\n      dispatch(setItemInCart(info));\n    } catch (error) {\n      console.error(error.message);\n    }\n  }\n\n  const getProduct = useCallback(async () => {\n    const products = await getSelectedProduct(productId);\n    setProduct(products);\n  }, [setProduct]);\n\n  useEffect(() => {\n    getProduct();\n  }, []);\n\n  return (\n    <React.Fragment>\n      {!!product.name && (\n        <div>\n          <div className={classes.product__header}>\n            <p>\n              <Link to=\"/\">Home</Link>/ <Link to=\"/products\">Books</Link>/\n              {product.name}\n            </p>\n          </div>\n\n          <div className={classes.product_block}>\n            <div className={classes.product_block__main}>\n              <div className={classes.title_block}>\n                <ProductTitle\n                  className={classes.product_info__title}\n                  title={product.name}\n                />\n                <ProductAutor\n                  className={classes.product_info__author}\n                  author={product.author}\n                />\n              </div>\n\n              <div className={classes.product_info}>\n                <ProductDescription\n                  className={classes.product_info__text}\n                  description={product.description}\n                />\n                <ProductPriceBlock product={product} />\n\n                <div className={classes.product_button}>\n                  <Button\n                    type=\"main\"\n                    onClick={() => {\n                      addToCart(product);\n                    }}\n                  >\n                    <i className=\"fas fa-shopping-cart\"></i>\n                  </Button>\n\n                  <Button\n                    type=\"main\"\n                    // onClick={() => {\n                    //   addToFavorite(product);\n                    // }}\n                  >\n                    <i className=\"fas fa-heart\"></i>\n                  </Button>\n                </div>\n              </div>\n\n              <div className={classes.product_img}>\n                <div className={classes.product_img__large}>\n                  <ProductImg\n                    className={classes.gallery_item__large}\n                    item={product.imageUrls[toggle]}\n                    alt={product.name}\n                  />\n                </div>\n\n                <div className={classes.gallery}>\n                  {product.imageUrls.map((item, index) => (\n                    <ProductImg\n                      key={index}\n                      className={classes.gallery__item}\n                      item={item}\n                      alt={product.name}\n                      onClick={() => setToggle(index)}\n                    />\n                  ))}\n                </div>\n              </div>\n            </div>\n\n            <h3 className={classes.product_block__title}>Details</h3>\n\n            <ProductDetails product={product} />\n\n            <h3 className={classes.product_block__title} id=\"review\">\n              Customer Reviews\n            </h3>\n\n            <div className={classes.product_block__review}>\n              <form>\n                <textarea\n                  id=\"review\"\n                  name=\"Textarea\"\n                  placeholder=\"Please, live your comment here...\"\n                ></textarea>\n                <div className={classes.review__buttons}>\n                  <Button\n                    type=\"main\"\n                    size=\"m\"\n                    // onClick={() => {\n                    // }}\n                  >\n                    Reset\n                  </Button>\n                  <Button\n                    type=\"main\"\n                    size=\"m\"\n                    // onClick={() => {\n                    // }}\n                  >\n                    Send\n                  </Button>\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n      )}\n    </React.Fragment>\n  );\n};\n\nexport default Product;\n","/home/tirion/Desktop/Final_project/client/src/api/productsApi.js",[],"/home/tirion/Desktop/Final_project/client/src/api/userApi.js",[],"/home/tirion/Desktop/Final_project/client/src/store/selectors.js",[],"/home/tirion/Desktop/Final_project/client/src/components/Categories/CategoriesList.js",["265"],"import React, { useCallback, useState, useEffect } from \"react\";\nimport CategoriesItem from \"./CategoriesItem\";\nimport { getAllCategories } from \"../../api/catalogApi\";\n\nconst CategoriesList = () => {\n  const [category, setCategory] = useState([]);\n  const getCategories = useCallback(async () => {\n    const categories = await getAllCategories();\n    setCategory(categories);\n  }, [setCategory]);\n\n  useEffect(() => {\